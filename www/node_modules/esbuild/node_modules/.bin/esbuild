#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.31/node_modules/esbuild/bin/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.31/node_modules/esbuild/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.31/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/node_modules:/mnt/c/Users/Linken/Videos/product/rust/node_modules:/mnt/c/Users/Linken/Videos/product/node_modules:/mnt/c/Users/Linken/Videos/node_modules:/mnt/c/Users/Linken/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.31/node_modules/esbuild/bin/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.31/node_modules/esbuild/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.31/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules/.pnpm/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/www/node_modules:/mnt/c/Users/Linken/Videos/product/rust/wasm-game/node_modules:/mnt/c/Users/Linken/Videos/product/rust/node_modules:/mnt/c/Users/Linken/Videos/product/node_modules:/mnt/c/Users/Linken/Videos/node_modules:/mnt/c/Users/Linken/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
